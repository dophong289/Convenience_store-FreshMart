{
  "java.enabled": false,
  "java.jdt.ls.java.home": null,
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.compile.nullAnalysis.mode": "disabled",
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.import.gradle.enabled": false,
  "java.autobuild.enabled": false,
  "java.maxConcurrentBuilds": 1,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/backend-springboot/target": true,
    "**/backend-springboot/.classpath": true,
    "**/backend-springboot/.project": true,
    "**/backend-springboot/.settings": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/target": true,
    "**/*.code-search": true
  },
  "files.associations": {
    "*.java": "plaintext"
  }
}

